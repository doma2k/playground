{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "comma-dangle": 0,
    "new-cap": 0,
    "no-nested-ternary": 0,
    "no-shadow": 0,
    "no-return-assign": 0,
    "react/jsx-no-bind": [1, {
    "ignoreRefs": true,
    "allowArrowFunctions": true,
    "allowBind": true,
    }],
    "max-len": 0,
    "no-param-reassign": [2, { "props": false }]
  }
}
